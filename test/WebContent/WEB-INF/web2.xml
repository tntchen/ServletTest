	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Demo4Filter</display-name>
		<servlet-name>Demo4Filter</servlet-name>
		<servlet-class>com.runoob.webServlet.Demo4Filter</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Demo4Filter</servlet-name>
		<url-pattern>/Demo4Filter</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/ErrorHandler</location>
	</error-page>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/ErrorHandler</location>
	</error-page>

</web-app>