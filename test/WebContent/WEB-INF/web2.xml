		<servlet-class>com.tnttest.hello.ReadParams</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReadParams</servlet-name>
		<url-pattern>/ReadParams</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DisplayHeader</display-name>
		<servlet-name>DisplayHeader</servlet-name>
		<servlet-class>com.runoob.httpservlet.DisplayHeader</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayHeader</servlet-name>
		<url-pattern>/DisplayHeader</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Refresh</display-name>
		<servlet-name>Refresh</servlet-name>
		<servlet-class>com.runoob.httpservlet.Refresh</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Refresh</servlet-name>
		<url-pattern>/Refresh</url-pattern>
	</servlet-mapping>
	<servlet>
